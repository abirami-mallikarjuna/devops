pipeline{
    agent any
    
    environment{
            PATH = "/maven/bin:$PATH"
    }
    stages{
        stage("Git checkout"){
            steps{
                git credentialsId: 'eb6f4a3a-a221-46ef-afc6-18d3fcdf7354', url: 'https://github.com/abirami-mallikarjuna/devops.git'
            }
        }
        stage("maven build"){
           steps{
               sh '''
               mvn clean 
               '''
           }
        }
     }
} 
